<body>

<script>

//check if a number is prime or not

let num=17;//you can change the number

let isPrime=true;

if(num<=1){
isPrime= false;

}else {
for(leti=2;<=Math.sqrt(num);i++)(num){
    if(num % i==0){
     isprime= false;
    break;
   }
 }
}
if (isPrime){
  console.log(num+"is a prime number");
} else {
   console.log(num+"is not a prime number");
}
</script>

